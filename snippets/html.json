{
    "sip-contextmenu": {
        "prefix": "sip-contextmenu",
        "body": [
            "<sip-contextmenu>",
            "</sip-contextmenu>$0"
        ],
        "description": "contextmenu"
    },
    "sip-template": {
        "prefix": "sip-template",
        "body": [
            "<ng-template>",
            "</ng-template>$0"
        ],
        "description": "template"
    },
    "sip-template-outlet": {
        "prefix": "sip-template-outlet",
        "body": [
            "[ngTemplateOutlet]=\"${tmpl1}\" [ngTemplateOutletContext]=\"{row:row, column:column, data:row.data}\"$0"
        ],
        "description": "[ngTemplateOutlet]"
    },
    "sip-template-if": {
        "prefix": "sip-template-if",
        "body": [
            "[ngIf]=\"${isOk}\"$0"
        ],
        "description": "[ngIf]"
    },
    "sip-template-for": {
        "prefix": "sip-template-for",
        "body": [
            "ngFor let-${item} [ngForOf]=\"${list}\""
        ],
        "description": "ngFor"
    },
    "sip-container": {
        "prefix": "sip-container",
        "body": [
            "<ng-container></ng-container>$0"
        ],
        "description": "container"
    },
    "sip-container-outlet": {
        "prefix": "sip-container-outlet",
        "body": [
            "[ngTemplateOutlet]=\"column.formatter\" [ngTemplateOutletContext]=\"{row:row, column:column, data:row.data}\"$0"
        ],
        "description": "container-outlet"
    },
    "sip-search-conent": {
        "prefix": "sip-search-conent",
        "body": [
            "<sip-search-conent [params]=\"${searchContent}\"></sip-search-conent>$0"
        ],
        "description": "<sip-search-conent>"
    },
    "sip-page": {
        "prefix": "sip-page",
        "body": [
            "<sip-page>",
            "\t<sip-page-header navigator=\"导航标题\">",
            "\t\t<ng-template #title>页面标题</ng-template>",
            "\t\t<ng-template #desc>",
            "\t\t\t页面描述",
            "\t\t</ng-template>",
            "\t</sip-page-header>",
            "\t<sip-page-body>",
            "\t\t页面内容",
            "\t</sip-page-body>",
            "</sip-page>$0"
        ],
        "description": "page"
    },
    "sip-page-header": {
        "prefix": "sip-page-header",
        "body": [
            "<sip-page-header navigator=\"导航标题\">",
            "\t<ng-template #title>页面标题</ng-template>",
            "\t<ng-template #desc>",
            "\t\t页面描述",
            "\t</ng-template>",
            "</sip-page-header>$0"
        ],
        "description": "page-header"
    },
    "sip-page-body": {
        "prefix": "sip-page-body",
        "body": [
            "<sip-page-body>",
            "\t页面内容",
            "</sip-page-body>$0"
        ],
        "description": "page-body"
    },
    "sip-page-row": {
        "prefix": "sip-page-row",
        "body": [
            "<div nz-row [nzGutter]=\"24\">$0",
            "</div>"
        ],
        "description": "page-row"
    },
    "sip-page-col": {
        "prefix": "sip-page-col",
        "body": [
            "<div nz-col [nzXl]=\"18\" [nzMd]=\"16\" >$0",
            "</div>"
        ],
        "description": "page-col"
    },
    "sip-page-toolbar": {
        "prefix": "sip-page-toolbar",
        "body": [
            "<sip-page-toolbar>",
            "\t<button nz-button (click)=\"refresh()\" [nzType]=\"'default'\" class=\"mr-sm\">",
            "\t\t<i class=\"anticon anticon-reload\"></i>",
            "\t</button>",
            "\t<button nz-button (click)=\"add()\" [nzType]=\"'primary'\" class=\"mr-sm\">",
            "\t\t<i class=\"anticon anticon-plus\"></i><span>新建</span>",
            "\t</button>",
            "\t<nz-dropdown [nzTrigger]=\"'hover'\" [nzPlacement]=\"'bottomLeft'\" class=\"mr-sm\">",
            "\t\t<button nz-button nz-dropdown ><i class=\"fa fa-ellipsis-v mr-sm\"></i><span>更多</span> <i class=\"anticon anticon-down\"></i></button>",
            "\t\t<ul nz-menu>",
            "\t\t\t<li nz-menu-item>重启</li>",
            "\t\t\t<li nz-menu-item><a [routerLink]>修改IP绑定</a></li>",
            "\t\t\t<li nz-menu-item><a (click)=\"expression\">挂接存储</a></li>",
            "\t\t\t<li nz-menu-divider></li>",
            "\t\t\t<li nz-menu-item><a [routerLink]>获取密码</a></li>",
            "\t\t\t<li nz-menu-item><a (click)=\"expression\">重置密码</a></li>",
            "\t\t\t<li nz-menu-divider></li>",
            "\t\t\t<li nz-submenu>",
            "\t\t\t\t<span title>更多</span>",
            "\t\t\t\t<ul>",
            "\t\t\t\t\t<li nz-menu-item><a [routerLink]>迁移实例</a></li>",
            "\t\t\t\t\t<li nz-menu-item><a (click)=\"expression\">资源标签</a></li>",
            "\t\t\t\t</ul>",
            "\t\t\t</li>",
            "\t\t</ul>",
            "\t</nz-dropdown>",
            "\t<nz-input [(ngModel)]=\"searchParams.Content\" (click)=\"search()\" name=\"Content\" [nzType]=\"'search'\" [nzPlaceHolder]=\"'请输入'\"  style=\"width:220px;vertical-align: bottom;\"></nz-input>",
            "</sip-page-toolbar>$0"
        ],
        "description": "page-toolbar"
    },
    "sip-table": {
        "prefix": "sip-table",
        "body": [
            "<sip-table [manager]=\"tableManager\"></sip-table>$0"
        ],
        "description": "<sip-table>"
    },
    "sip-table-template": {
        "prefix": "sip-table-template",
        "body": [
            "<ng-template #tmplName let-row=\"row\" let-value=\"value\" let-column=\"column\">",
            "  <a [sipRouterLink]=\"['/sip-online/user/user-form', {id:row.\\$\\$data.id}]\">{{value}}</a>",
            "</ng-template>$0"
        ],
        "description": "<template>"
    },
    "sip-modal-header": {
        "prefix": "sip-modal-header",
        "body": [
            "<sip-modal-header>",
            "    <span>标题 - [ {{params.id}} ]</span>",
            "</sip-modal-header>"
        ],
        "description": "modal-header"
    },
    "sip-modal-body": {
        "prefix": "sip-modal-body",
        "body": [
            "$0<sip-modal-body>",
            "    <div style=\"width:1000px\">",
            "        <p>对话框的内容</p>",
            "        <p>对话框的内容</p>",
            "    </div>",
            "</sip-modal-body>"
        ],
        "description": "modal-body"
    },
    "sip-modal-footer": {
        "prefix": "sip-modal-footer",
        "body": [
            "$0<sip-modal-footer>",
            "    <button nz-button [nzType]=\"'primary'\" [nzSize]=\"'large'\" (click)=\"save(\\$event)\" [nzLoading]=\"false\">",
            "        提 交",
            "    </button>",
            "</sip-modal-footer>"
        ],
        "description": "modal-footer"
    },
    "sip-access": {
        "prefix": "sip-access",
        "body": [
            "sipAccess=\"${create}\"$0"
        ],
        "description": "sipAccess"
    },
    "sip-RouterLink": {
        "prefix": "sip-RouterLink",
        "body": [
            "[sipRouterLink]=\"['$0/sip/ui-demo/detail', {}]\""
        ],
        "description": "[sipRouterLink]"
    },
    "sip-show": {
        "prefix": "sip-show",
        "body": [
            "[sipShow]=\"true\""
        ],
        "description": "[sipShow]"
    },
    "sip-form": {
        "prefix": "sip-form",
        "body": [
            "<form nz-form [formGroup]=\"form\">$0",
            "</form>"
        ],
        "description": "<form>"
    },
    "sip-form-item": {
        "prefix": "sip-form-item",
        "body": [
            "<nz-form-item>$0",
            "</nz-form-item>"
        ],
        "description": "<sip-form-item>"
    },
    "sip-form-label": {
        "prefix": "sip-form-label",
        "body": [
            "<nz-form-label nzXs=\"24\" nzSm=\"4\" nzRequired>${label}</nz-form-label>$0"
        ],
        "description": "<nz-form-label>"
    },
    "sip-form-control": {
        "prefix": "sip-form-control",
        "body": [
            "<nz-form-control nzXs=\"24\" nzSm=\"18\">",
            "   ${container}$0",
            "</nz-form-control>"
        ],
        "description": "<form-control>"
    },
    "sip-form-explain": {
        "prefix": "sip-form-explain",
        "body": [
            "<nz-form-explain *ngIf=\"form.get('${name}').dirty && form.get('${name}').errors\">${请输入}</nz-form-explain>$0"
        ],
        "description": "<form-explain>"
    },
    "sip-form-input": {
        "prefix": "sip-form-input",
        "body": [
            "<input nz-input formControlName=\"${name}\" placeholder=\"${给目标起个名字}\">$0"
        ],
        "description": "input"
    },
    "sip-form-input-group": {
        "prefix": "sip-form-input-group",
        "body": [
            "<nz-input-group [nzCompact]=\"true\">",
            "    <nz-select formControlName=\"${name}Prefix\" style=\"width: 25%;\">",
            "        <nz-option [nzLabel]=\"'+86'\" [nzValue]=\"'+86'\"></nz-option>",
            "        <nz-option [nzLabel]=\"'+87'\" [nzValue]=\"'+87'\"></nz-option>",
            "    </nz-select>",
            "    <input formControlName=\"${name}\" nz-input style=\"width: 75%;\">",
            "</nz-input-group>$0"
        ],
        "description": "input-group"
    },
    "sip-form-select": {
        "prefix": "sip-form-select",
        "body": [
            "<nz-select formControlName=\"${name}\" [nzAllowClear]=\"false\" nzShowSearch>",
            "    <nz-option *ngFor=\"let item of statuList\" [nzLabel]=\"item.text\" [nzValue]=\"item.value\"></nz-option>$0",
            "</nz-select>"
        ],
        "description": "select"
    },
    "sip-form-select-multiple": {
        "prefix": "sip-form-select-multiple",
        "body": [
            "<nz-select formControlName=\"${name}\" [nzAllowClear]=\"false\" nzMode=\"multiple\" nzNotFoundContent=\"无法找到\">",
            "    <nz-option *ngFor=\"let item of statuList\" [nzLabel]=\"item.text\" [nzValue]=\"item.value\"></nz-option>$0",
            "</nz-select>"
        ],
        "description": "select-multiple"
    },
    "sip-form-radio-group": {
        "prefix": "sip-form-radio-group",
        "body": [
            "<nz-radio-group formControlName=\"${name}\">",
            "    <label nz-radio [nzValue]=\"'normal'\">",
            "        <span>Normal</span>",
            "    </label>$0",
            "</nz-radio-group>"
        ],
        "description": "radio-group"
    },
    "sip-form-radio-group-button": {
        "prefix": "sip-form-radio-group-button",
        "body": [
            "<nz-radio-group formControlName=\"${name}\">",
            "    <label nz-radio-button [nzValue]=\"'normal'\">",
            "        <span>Normal</span>",
            "    </label>$0",
            "</nz-radio-group>"
        ],
        "description": "radio-group-button"
    },
    "sip-form-text": {
        "prefix": "sip-form-text",
        "body": [
            "<div nz-form-extra>${text}$0</div>"
        ],
        "description": "form-text"
    },
    "sip-form-checkbox": {
        "prefix": "sip-form-checkbox",
        "body": [
            "<label nz-checkbox formControlName=\"${name}\">",
            "    <span>${text}$0</span>",
            "</label>"
        ],
        "description": "checkbox"
    },
    "sip-form-datepicker": {
        "prefix": "sip-form-datepicker",
        "body": [
            "<nz-date-picker formControlName=\"${name}\"></nz-date-picker>"
        ],
        "description": "<nz-date-picker>"
    },
    "sip-form-datepicker-range": {
        "prefix": "sip-datepicker-range",
        "body": [
            "<nz-range-picker formControlName=\"${name}\"></nz-range-picker>"
        ],
        "description": "<nz-range-picker>"
    },
    "sip-form-textarea": {
        "prefix": "sip-form-textarea",
        "body": [
            "<textarea nz-input formControlName=\"${name}\" [nzAutosize]=\"{minRows: 4}\" placeholder=\"${placeholder}\"></textarea>$0"
        ],
        "description": "textarea"
    },
    "sip-form-divider": {
        "prefix": "sip-form-divider",
        "body": [
            "<nz-divider nzTitle=\"\" nzDashed></nz-divider>$0"
        ],
        "description": "divider"
    },
    "sip-form-controlName": {
        "prefix": "sip-form-controlName",
        "body": [
            "formControlName=\"${name}\"$0"
        ],
        "description": "controlName"
    }
}